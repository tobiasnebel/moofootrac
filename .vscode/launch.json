    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'models'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=models"
                ],
                "filter": {
                    "name": "models",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'moofoolog' [dev]",
            "cargo": {
                "args": [
                    "build",
                    "--bin=moofoolog",
                    "--package=moofoolog"
                ],
                "filter": {
                    "name": "moofoolog",
                    "kind": "bin"
                }
            },
            "args": ["--config=config/dev.yml"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'moofoolog' [default]",
            "cargo": {
                "args": [
                    "build",
                    "--bin=moofoolog",
                    "--package=moofoolog"
                ],
                "filter": {
                    "name": "moofoolog",
                    "kind": "bin"
                }
            },
            // "args": ["--config=config/default.yml"],
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'moofoolog'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=moofoolog",
                    "--package=moofoolog"
                ],
                "filter": {
                    "name": "moofoolog",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}