server {
    server_name moofoolog.nebeltobias.de;

    access_log off;
    error_log /var/log/nginx/moofoolog.nebeltobias.de.error.log;

    location = / {
      return 301 /app/;
    }

    location = /app {
      return 301 /app/;
    }

    location /app/ {
        proxy_pass          http://127.0.0.1:9011/app/;
        proxy_http_version 1.1;

        # Standard Proxy Headers
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
    }

    location /api/ {
        proxy_pass          http://127.0.0.1:9011/api/;
        proxy_http_version 1.1;

        # Standard Proxy Headers
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
    }


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/moofoolog.nebeltobias.de/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/moofoolog.nebeltobias.de/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = moofoolog.nebeltobias.de) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name moofoolog.nebeltobias.de;
    return 404; # managed by Certbot


}